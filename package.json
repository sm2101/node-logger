{
  "name": "@webxsid/node-logger",
  "version": "1.0.0",
  "description": "An all in one logger for nodejs",
  "main": "./dist/bin/app.js",
  "types": "./dist/bin/app.d.ts",
  "author": "Siddharth Mittal<hello@webxsid.com>",
  "license": "MIT",
  "bin": {
    "logger": "./dist/bin/app.js"
  },
  "keywords": [
    "logger",
    "nodejs",
    "typescript",
    "node-logger"
  ],
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:dev": "node ./scripts/pre-build.js dev && tsc",
    "build:test": "node ./scripts/pre-build.js test && tsc",
    "build": "node ./scripts/pre-build.js prod && tsc",
    "dev": "node ./scripts/pre-build.js dev && tsc -w",
    "prepare": "npm run build",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "concurrently": "^8.2.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "socket.io-client": "^4.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
